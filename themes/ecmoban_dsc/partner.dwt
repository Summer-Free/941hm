        <script type="text/javascript">
            $(function(){
                $("#check_submit").click(function(e){
                        e.preventDefault();
                        var bm_name = $("#bm_name").val(),
                            bm_phone = $("#bm_phone").val(),
                            address = $("#address").val(),
                            reg = /^[\u4e00-\u9fa5]{2,4}$/i;
                        if (!reg.test(bm_name))
                            {
                                alert("请输入2-4个汉字！"); 
                                return false;
                            }
                        else if(!(/^1[34578]\d{9}$/.test(bm_phone)))
                            { 
                                alert("手机号码有误，请重填");  
                                return false;
                            }
                        else if(address == "")
