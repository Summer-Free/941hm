	<script type="text/javascript" src="js/jquery.lazyload.js"></script>
	<!--开启懒加载-->
    <script type="text/javascript">
        $("img.lazy").lazyload({
            placeholder: "themes/ecmoban_dsc/images/loadGoods.gif",
            effect : "fadeIn",
            threshold : 1000
        });
        function active_pro(id)
        {
            var goods_id = id;
            $.ajax({
			   type: "POST",
			   url: "homedress.php",
			   data: {goods_id:goods_id},
			   dataType:'json',
			   success: function(data){
			   	//console.log(data);
                    //1=已结束
                    //2=未开始
                    if (data.id == 1) {
                        $(".homedress_tip_end").show();
                        $(".homedress_tip_end").attr("alt",data.goods_id);
                        // data.goods_id
                        $(".homedress_tip_close").show();
                        $(".homedress_tip").show();
                    }else{
                        $(".homedress_tip_start").show();
                        $(".homedress_tip_start").attr("alt",data.goods_id);
                        console.log($(".homedress_tip_start").attr("alt"));
                        $(".homedress_tip_close").show();
                        $(".homedress_tip").show();
                    }
			   	}
			});
        };
        function close_tip(){
			$(".homedress_tip_end").hide();
			$(".homedress_tip_start").hide();
			$(".homedress_tip_close").hide();
			$(".homedress_tip").hide();
		};
		function location_end1(){
			var goods_id =$(".homedress_tip_end").attr('alt');
			window.location.href = 'goods.php?id='+goods_id;
			
		}
		function location_start1(){
			var goods_id =$(".homedress_tip_start").attr('alt');
			window.location.href = 'goods.php?id='+goods_id;
			
		}
    </script>
