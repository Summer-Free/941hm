        <script type="text/javascript">
        	$(function() {

        		$('.sec-2 li img').on("click",function() {
        			var $this = $(this),
        				str = $this.attr("src"),
        				id=$this.attr("cou_id"),
        				arr = str.split(""),
        				index = str.indexOf("."),
        				src = (str.indexOf("-1") != -1)?arr.slice(0,index).join("") + ".png":arr.slice(0,index).join("") + "-1.png";
        				if({$smarty.session.user_id} == 0){
							window.location.href="user.php?act=active";
						}else{
							$.post('coupons.php?act=coupons_receive',{'cou_id':id},function(data){
								if(data.status=='ok'){
									$this.attr("src",src);
								}else{
									alert ('您已领取过优惠券，请勿重复领取');
									$this.attr("src",src);
								}
							},'json')
						}
						
        		});
			});
        </script>
